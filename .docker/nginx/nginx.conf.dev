server {
    listen 80;

    server_name farsh-mazhor.local;

    location / {
        proxy_pass http://frontend:4000/;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";

    }
    location /api/v1 {
        proxy_pass http://api:3000;
    }
}